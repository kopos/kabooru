<td id="left">
</td>
<td id="center">
  <h1>Listing users</h1>

  <table>
    <tr>
      <th>Email</th>
      <th>Name</th>
      <th>First name</th>
      <th>Last name</th>
      <th>Telephone</th>
      <th>Job title</th>
      <th>Gmail</th>
      <th>Yahoo</th>
      <th>Skype</th>
    </tr>

  <% for user in @users %>
    <tr>
      <td><%=h user.email_id %></td>
      <td><%=h user.name %></td>
      <td><%=h user.first_name %></td>
      <td><%=h user.last_name %></td>
      <td><%=h user.telephone %></td>
      <td><%=h user.job_title %></td>
      <td><%=h user.gmail_id %></td>
      <td><%=h user.yahoo_id %></td>
      <td><%=h user.skype_id %></td>
      <td><%= link_to 'Show', user %></td>
      <td><%= link_to 'Edit', edit_user_path(user) %></td>
      <td><%= link_to 'Destroy', user, :confirm => 'Are you sure?', :method => :delete %></td>
    </tr>
  <% end %>
  </table>

  <br />

  <%= link_to 'New user', new_user_path %>
</td>
<td id="right"></td>